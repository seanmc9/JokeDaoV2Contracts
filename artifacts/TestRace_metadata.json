			"methods": {
				"COUNTING_MODE()": {
					"details": "See {IGovernor-COUNTING_MODE}. "
				},
				"cancel()": {
					"details": "Internal cancel mechanism: locks up the proposal timer, preventing it from being re-submitted. Marks it as canceled to allow distinguishing it from executed proposals. Emits a {IGovernor-ProposalCanceled} event."
				},
				"castVote(uint256,uint8,uint256)": {
					"details": "See {IGovernor-castVote}."
				},
				"castVoteBySig(uint256,uint8,uint256,uint8,bytes32,bytes32)": {
